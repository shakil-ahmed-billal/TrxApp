{
  "version": 2,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "dist/src/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/generated/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/server.js"
    }
  ]
}